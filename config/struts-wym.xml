<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="jypx_default" namespace="/jypx" extends="struts-default">
		<action name="list" class="com.web.front.action.JypxAction"
			method="list">
			<result>/pages/front/jypx/index.jsp</result>
		</action>
		<action name="save" class="com.web.front.action.JypxAction"
			method="save">
			<result type="redirect">/jypx.aspx</result>
		</action>
		<action name="uploadPage" class="com.web.front.action.JypxAction"
			method="gotoUploadPage">
			<result>/pages/front/jypx/upload.jsp</result>
		</action>
	</package>
	
	<package name="xbkc_default" namespace="/xbkc" extends="struts-default">
		<action name="list" class="com.web.front.action.XbkcAction"
			method="list">
			<result>/pages/front/xbkc/index.jsp</result>
		</action>
		<action name="save" class="com.web.front.action.XbkcAction"
			method="save">
			<result type="redirect">/xbkc.aspx</result>
		</action>
		<action name="uploadPage" class="com.web.front.action.XbkcAction"
			method="gotoUploadPage">
			<result>/pages/front/xbkc/upload.jsp</result>
		</action>
	</package>
</struts>
